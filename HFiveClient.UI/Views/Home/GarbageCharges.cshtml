
@{
    ViewBag.Title = "垃圾费用";
}
@section  styles{
    <style>

        body {
            font-family: 'Microsoft YaHei, Helvetica Neue,Helvetica,Arial,sans-serif';
            background-color: #efefef;
        }

        .weui-media-box_text .weui-media-box__title {
            text-align: center;
        }

        .bggrey {
            background-color: #efefef;
        }


        .bgwhite {
            background-color: white;
        }

        header {
            background: -moz-linear-gradient(top, #efefef 0%, #efefef 100%);
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#efefef), color-stop(100%,#efefef));
            background: -webkit-linear-gradient(top, #efefef 0%,#efefef 100%);
            background: -o-linear-gradient(top, #efefef 0%,#efefef 100%);
            background: -ms-linear-gradient(top, #efefef 0%,#efefef 100%);
            background: linear-gradient(to bottom, #efefef 0%,#efefef 100%);
        }

        .box {
            display: flex;
            justify-content: center;
        }


        .burlywood {
            color: burlywood
        }

        .weui-cell.nopd {
            border-bottom: none;
        }

        .ellipse {
            text-align: center;
            border-radius: 10px;
            margin: 0 auto;
            width: 8em;
            height: 2em;
            line-height: 2em;
            background: #999;
        }

            .ellipse > a {
                color: #ff9900;
            }

                .ellipse > a:first-child {
                    float: left;
                    width: 50%;
                    height: 2em;
                    color: white;
                }

        .hotlight {
            background-color: #ff9900;
        }

        .weui-grid {
            padding: 5px;
        }

            .weui-grid:before, .weui-grid:after {
                border: none;
            }

        .weui-panel__hd i {
            float: right;
            width: 1em;
            height: 1em;
            color:#666;
        }
    </style>

}
<header class="bggrey">
    <div class="wy-header-icon-back bggrey"><span></span></div>
    <div class="wy-header-title bggrey">垃圾费用.伶俐服务区</div>
</header>


<div class="weui-panel__bd bgwhite" style="margin:1em;">
    <div class="weui-media-box weui-media-box_text bgwhite">
